{
    "manifest_version": 2,
    "name": "G-Tasks Popup",
    "author": "DarkMannn",
    "version": "1.0.0",
    "description": "Google Tasks extension for keyboard users",
    "key": "",
    "oauth2": {
        "client_id": "128290994570-7ot1lghfc5r90pqgutsevaliou717c0j.apps.googleusercontent.com",
        "scopes": ["https://www.googleapis.com/auth/tasks"]
    },
    "permissions": [
        "identity"
    ],
    "icons": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    },
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Open G-Tasks Popup"
    },
    "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}
